{
  "name": "gurubot",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog",
    "dev": "tsx src/main.ts",
    "start": "node build/main.js",
    "start:migrate": "prisma migrate deploy && npm run start",
    "watch": "nodemon --exec tsx src/main.ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.2.2",
    "@discordx/pagination": "^3.4.1",
    "@prisma/client": "^4.16.1",
    "discord.js": "^14.11.0",
    "discordx": "^11.7.6",
    "tslog": "^4.8.2",
    "prisma": "^4.16.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "5.1.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
